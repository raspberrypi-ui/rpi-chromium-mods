#!/bin/sh

ARCH=$(dpkg --print-architecture)

if [ "$ARCH" !=  "armhf" ] ; then
	dpkg-divert --package rpi-chromium-mods --divert /etc/chromium/master_preferences.bak --rename /etc/chromium/master_preferences
	dpkg-maintscript-helper rm_conffile /usr/share/chromium/master_preferences 20190912 -- "$@"
fi

