#!/bin/sh

ARCH=$(dpkg --print-architecture)

if [ "$ARCH" !=  "armhf" ] ; then
	if [ remove = "$1" -o abort-install = "$1" -o disappear = "$1" ]; then
		dpkg-divert --package rpi-chromium-mods --rename --remove /etc/chromium/master_preferences
	fi
	dpkg-maintscript-helper rm_conffile /usr/share/chromium/master_preferences 20190912 -- "$@"
fi

